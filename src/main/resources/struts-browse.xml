<struts>
	<package name="CONGO-browse" extends="CONGO-main">
	
	<action name="coconut/processBrowse" method="process" class="com.stonekeep.congo.coconut.Browse">
		<interceptor-ref name="mystack" />
		<result name="exit" type="redirectAction">coconut/Home</result>
